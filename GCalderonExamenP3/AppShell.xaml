<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GCalderonExamenP3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GCalderonExamenP3"
    Shell.FlyoutBehavior="Disabled"
    Title="GCalderonExamenP3"
    FlyoutBackgroundColor="DarkCyan">

    <TabBar>
        <ShellContent
    Title="BuscarPais"
    ContentTemplate="{DataTemplate local:PaisView}"
    Route="PaisView" />
        <ShellContent
            Title="Listado de paises"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        
    </TabBar>
        

</Shell>
